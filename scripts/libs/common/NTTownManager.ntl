			if(_NTTMGR_DoID || _NTTMGR_BuyPots || _NTTMGR_BuyTP || _NTTMGR_BuyKey)
			{
				if(NTT_DoTrade(_npc))
				{
					if(_NTTMGR_BuyPots)
						NTT_FillBelt(_npc);
					if(_NTTMGR_BuyTP)
						NTT_FillTome(_npc, 1);
					if(_NTTMGR_BuyKey)
